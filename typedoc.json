{
  "$schema": "https://typedoc.org/schema.json",
  "includeVersion": true,
  "entryPoints": ["src/index.ts"],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mdn-links"],
  "exclude": [
    "**/test/**/*",
    "**/dist/**/*",
    "**/lib/**/*",
    "**/shims/**/*",
    "**/node_modules/**/*",
    "**/coverage/**/*"
  ],
  "cleanOutputDir": true,
  "readme": "none",
  "out": "./docs",
  "excludeReferences": true,
  "logLevel": "Verbose"
}
